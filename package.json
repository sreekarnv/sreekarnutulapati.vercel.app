{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "next dev",
    "gen:sass:types": "typed-scss-modules src -w",
    "dev": "npm-run-all -p serve gen:sass:types",
    "build": "next build && yarn generate",
    "start": "next start",
    "lint": "next lint",
    "prettier:check": "prettier --check ./",
    "prettier:write": "prettier --write ./",
    "prepare": "husky install",
    "generate:sitemap": "node ./scripts/generate-sitemap.js",
    "generate:robotstxt": "node ./scripts/generate-robots-txt.js",
    "generate": "npm-run-all generate:sitemap generate:robotstxt"
  },
  "dependencies": {
    "@hookform/resolvers": "^2.9.10",
    "@next/font": "^13.1.6",
    "@plaiceholder/next": "^2.5.0",
    "@web3forms/react": "^1.1.3",
    "clsx": "^1.2.1",
    "dotenv": "^16.0.3",
    "framer-motion": "^7.6.2",
    "fuse.js": "^6.6.2",
    "gray-matter": "^4.0.3",
    "marked": "^4.1.1",
    "next": "^13.1.6",
    "nextjs-sitemap-generator": "^1.3.1",
    "plaiceholder": "^2.5.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.39.1",
    "react-icons": "^4.4.0",
    "react-toastify": "^9.1.1",
    "slugify": "^1.6.5",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@types/node": "18.8.2",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "eslint": "8.24.0",
    "eslint-config-next": "12.3.1",
    "husky": "^8.0.0",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "sass": "^1.55.0",
    "sharp": "^0.31.1",
    "typed-scss-modules": "^7.0.1",
    "typescript": "4.8.4"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "yarn eslint ./",
      "yarn prettier:check"
    ]
  },
  "packageManager": "yarn@3.2.4"
}
